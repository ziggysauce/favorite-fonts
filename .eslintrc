{
  "extends": [
    "eslint:recommended",
    "react-app",
    "airbnb",
    "airbnb/hooks",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "jsx-a11y",
    "prettier",
    "react"
  ],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": 0,
    "react/require-default-props": "off",
    "react/prop-types": "off",
    "prettier/prettier": [
      "error", {
        "semi": true,
        "singleQuote": true
      }
    ]
  }
}